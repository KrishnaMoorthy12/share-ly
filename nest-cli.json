{
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "plugins": [
    {
      "name": "@nestjs/swagger",
      "options": {
        "classValidatorShim": true,
        "introspectComments": true,
        "dtoFileNameSuffix": [
          "input.ts",
          "input.dto.ts",
          "output.ts",
          "output.dto.ts",
          "dto.ts",
          "entity.ts"
        ]
      }
    }
  ]
}
